<?xml version="1.0" encoding="utf-8" ?>
<component name="KeyboardItem" extends="Group">
 
    <interface>
        <field id="itemContent" type="node" onChange="onContentChanged" />
        <field id="focusPercent" type="float" onChange="itemPercentChanged" />
        <field id="gridHasFocus" type="boolean" onChange="onGridHasFocus" />
    </interface>

    <children>

        <Label 
            id="itemText"
            color="#FFFFFF"
            font="font:MediumBoldSystemFont" />
    </children>

    <script type="text/brightscript" uri="keyboardItem.brs" />
</component>
