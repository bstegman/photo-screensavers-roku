<?xml version="1.0" encoding="utf-8" ?>
<component name="ClockTextNode" extends="ContentNode">

	<interface>
		<!--
			font style
		-->
		<field id="style" type="string" value="Bonbon" />

		<!--
			supported values:
				time - 3:54 pm
				dateTime
					February 12, 2021
					Thursday 3:54 pm
		-->
		<field id="format" type="string" value="time" />

		<!--
			font color
		-->
		<field id="color" type="string" value="#FFFFFF" />
	</interface>
</component>
