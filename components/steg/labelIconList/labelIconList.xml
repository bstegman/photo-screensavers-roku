<component name="LabelIconList" extends="MarkupList">

	<interface>
     	<field id="itemContent" type="node" onChange="showContent" />
		<field id="focusPercent" type="float" onChange="showFocus" />
		<field id="listHasFocus" type="boolean" onChange="onFocusChange" />
		<field id="focusColor" type="string" value="#f39c12ff" />
		<field id="nonFocusColor" type="string" value="#ffffff" />
   </interface>

	<script type="text/brightscript" uri="labelIconList.brs" />
</component>
