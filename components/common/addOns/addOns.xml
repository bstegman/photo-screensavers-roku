<?xml version="1.0" encoding="utf-8" ?>
<component name="AddOns" extends="Group" >

	<interface>
        <field id="keepAliveEnabled" type="Boolean" value="false" />

        <function name="changeOpacity" />
		<function name="load" />
		<function name="loadFree" />
		<function name="loadMusic" />
		<function name="loadWidget" />
        <function name="pause" />
	</interface>

	<children>

        <Rectangle
            id="overlay"
            color="#000000"
            opacity=".60"
            visible="false" />

		<AnimationFalling 
            id="animationFalling"
            numItems="25" />

        <AudioPlaylist 
            id="audio"
            signUrls="true" />

        <KeepAlive 
            id="keepAlive" />

        <Timer
            id="weatherTimer"
            duration="900"
            repeat="false" />
	</children>

	<script type="text/brightscript" uri="addOns.brs" />
    <script type="text/brightscript" uri="pkg:/source/api/cloudfront.brs" />
    <script type="text/brightscript" uri="pkg:/source/api/music.brs" />
    <script type="text/brightscript" uri="pkg:/source/api/weather.brs" />
    <script type="text/brightscript" uri="pkg:/source/reg/clock.brs" />
    <script type="text/brightscript" uri="pkg:/source/reg/general.brs" />
    <script type="text/brightscript" uri="pkg:/source/reg/weather.brs" />
	<script type="text/brightscript" uri="pkg:/source/steg/api/utils.brs" />
    <script type="text/brightscript" uri="pkg:/source/steg/dateTime.brs" />
    <script type="text/brightscript" uri="pkg:/source/steg/registry.brs" />
	<script type="text/brightscript" uri="pkg:/source/steg/type.brs" />
	<script type="text/brightscript" uri="pkg:/source/steg/ui.brs" />
    <script type="text/brightscript" uri="pkg:/source/utils/api.brs" />
    <script type="text/brightscript" uri="pkg:/source/utils/weather.brs" />
</component>
